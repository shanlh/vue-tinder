{
  "name": "vue-tinder",
  "version": "3.0.0-alpha.2",
  "description": "Imitate Tinder by Vue.js",
  "author": {
    "name": "JohnnyDan",
    "email": "shanlh841271734@live.com"
  },
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "docs:build": "vuepress build docs",
    "docs:deploy": "bash deploy_docs.sh",
    "docs:dev": "vuepress dev docs",
    "release": "bash release.sh"
  },
  "main": "lib/vue-tinder.es.js",
  "dependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@vitejs/plugin-vue": "^1.1.2",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vuepress/plugin-google-analytics": "^1.2.0",
    "eslint": "^7.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-vue": "^7.2.0",
    "prettier": "^2.2.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "vite": "^2.0.0-beta.50",
    "vuepress": "^1.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "@vue/prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-console": 1,
      "vue/no-v-model-argument": 0
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "ignorePatterns": [
      "lib"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "babel": {
    "presets": [
      [
        "@vue/cli-plugin-babel/preset",
        {
          "useBuiltIns": false
        }
      ]
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "homepage": "https://github.com/shanlh/vue-tinder",
  "keywords": [
    "tinder",
    "tantan",
    "vue"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shanlh/vue-tinder.git"
  }
}
